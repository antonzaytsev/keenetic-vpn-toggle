# Keenetic Router Configuration
KEENETIC_HOST=192.168.1.1
KEENETIC_LOGIN=admin
KEENETIC_PASSWORD=your_password_here

# VPN Policy name as shown in Keenetic (e.g., !WG1, !OVP1)
VPN_POLICY=!WG1

# External ports (optional)
FRONTEND_PORT=3000
BACKEND_PORT=4567

# Backend host for frontend (optional, e.g., vpn-backend.internal)
# If set, BACKEND_PORT is ignored
BACKEND_HOST=

